{
    "name": "theranken/ectype",
    "description": "A package to supercharge your enums with labels, collections, and Laravel translation support",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ranken\\Ectype\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dev Ranken",
            "email": "rankencorp@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "suggest": {
        "laravel/framework": "Required for translation features (^9.0|^10.0|^11.0|^12.0)"
    }
}
